{
  "restartable": "rs",
  "ignore": [
    ".git",
    ".svn",
    "logs",
    "pem",
    "node_modules/**/node_modules",
    "./bin",
    "./client",
    "./server"
  ],
  "verbose": true,
  "execMap": {
    "js": "node static/app.js"
  },
  "watch": ["static/*"],
  "env": {
    "NODE_ENV": "production"
  },
  "ext": "ts js json",
  "exec": "node static/app.js"
}
