{
  "name": "@baicie/pm2-web",
  "version": "0.2.3",
  "description": "pm2 web server",
  "type": "commonjs",
  "author": "baicie",
  "files": [
    "static",
    "scripts",
    "bin",
    "nodemon.prod.json"
  ],
  "bin": {
    "pm2-web": "bin/pm2-web"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baicie/pm2-web"
  },
  "scripts": {
    "dev-server": "nodemon --config nodemon.json dev",
    "dev-client": "vite --config ./client/vite.config.ts",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "build-server": "tsup server/app.ts",
    "build-client": "vite build --config ./client/vite.config.ts",
    "build": "npm run build-client && npm run build-server",
    "static-app": "nodemon --config nodemon.json",
    "prod": "nodemon --config nodemon.prod.json",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "prepare": "husky install",
    "pub": "bumpp"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@hapi/boom": "^10.0.1",
    "@vue/shared": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "concurrently": "^7.6.0",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.33",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-validator": "^6.15.0",
    "fs-extra": "^11.1.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "pm2": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.1.4",
    "type-fest": "^3.6.1",
    "typeorm": "^0.3.12",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/types": "^17.4.4",
    "@swc/core": "^1.3.37",
    "@types/body-parser": "^1.19.2",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/i18n": "^0.13.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
